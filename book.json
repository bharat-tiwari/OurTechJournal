{
    "plugins": [ "theme-api", "jsfiddle"], 
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "jsfiddle":{
            "type":"script",
            "tabs":["result","js","css", "html"],
            "height": "500",
            "width": "500",
            "fontColor": "#1a1a1a",
            "bodyColor": "#444",
            "visual": "dark"
       }
    }
}

