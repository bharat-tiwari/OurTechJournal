{
    "plugins": [ "theme-api", "jsfiddle"], 
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark",
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "jsfiddle":{
            "type":"frame",
            "tabs":["html","js","css","result"],
            "height": "500",
            "width": "80%",
            "theme": "light",
            "menuColor": "b3cccc"
       }
    }
}

